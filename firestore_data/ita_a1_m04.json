    "liar_game_data": {
        "topic": "Italian False Friends",
        "question": "Your Italian friend says 'I miei parenti arrivano domani.' What does this mean?",
        "options": [
            "My parents are arriving tomorrow",
            "My relatives are arriving tomorrow",
            "My friends are arriving tomorrow",
            "My neighbors are arriving tomorrow"
        ],
        "correct_answer_index": 1,
        "explanation": "The Italian word 'parenti' is a false friend. It means 'relatives' in general (aunts, uncles, cousins, etc.), NOT specifically 'parents.' The Italian word for parents is 'genitori.' This is a very common mistake for English speakers learning Italian!"
    }

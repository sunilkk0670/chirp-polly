    "liar_game_data": {
        "topic": "Italian False Friends",
        "question": "Your Italian boss says 'Non puoi pretendere un aumento.' What is he saying?",
        "options": [
            "You cannot pretend to get a raise",
            "You cannot demand a raise",
            "You cannot expect a raise",
            "You cannot suggest a raise"
        ],
        "correct_answer_index": 1,
        "explanation": "The Italian verb 'pretendere' is a false friend. It means 'to demand' or 'to claim,' NOT 'to pretend' (which is 'fingere'). This can lead to very awkward misunderstandings in professional contexts!"
    }
}